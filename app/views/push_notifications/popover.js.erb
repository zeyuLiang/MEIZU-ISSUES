<% if !@changes.empty? || params[:refresh]=='1' %>
  $('#push_notification_popover ul.dropdown-menu').html("<%= j render('popover', { push_notifications: @push_notifications}) %>");
  _favicon.badge(<%= @unread_count %>);
  <% if @push_notifications.empty? %>
    $('#push_notification_popover').removeClass('has_notification');
  <% else %>
    $('#push_notification_popover').addClass('has_notification');
  <% end %>
<% else %>
  ;
<% end-%>
